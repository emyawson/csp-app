    "@ingka/skeleton": "^0.1.6",
    "@ingka/svg-icon": "^2.0.14",
    "@ingka/switch": "^0.3.19",
    "@ingka/toast": "^5.2.8",
    "@ingka/variables": "^3.0.2",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^12.0.0",
    "axios": "^0.21.1",
    "connected-react-router": "6.9.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "history": "4.10.1",
    "jest-environment-jsdom": "^27.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-helmet-async": "^1.0.9",
    "react-redux": "^7.2.4",
