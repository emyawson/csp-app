  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty",
      "pre-push": "yarn lint && yarn test:all"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
