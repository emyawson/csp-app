{
  "name": "customer-service-pages-frontend",
  "version": "0.0.0-development",
  "private": true,
  "scripts": {
    "dev": "nodemon server.js --watch build/*",
    "start": "yarn run clean && webpack serve  --mode development",
    "build": "yarn run clean && webpack --config webpack.prod.config.js --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:all": "jest --coverage --detectOpenHandles --passWithNoTests --silent",
    "cypress:open": "wait-on tcp:3000 && cypress open",
    "test:startdev": "yarn build && concurrently --kill-others \"yarn dev\"",
    "test:features": "yarn run test:startdev \"wait-on tcp:3000 && cypress open\"",
    "eject": "react-scripts eject",
    "pretty": "pretty-quick --staged",
    "lint": "eslint --ext .jsx --ext .js src",
    "lint:fix": "yarn lint --fix",
    "lint:test": "yarn lint && react-scripts test",
    "lint:css": "yarn stylelint './src/**/*.scss'",
    "clean": "rm -rf ./build",
    "lighthouse": "lighthouse https://www.cte.ikeadt.com/nl/nl/customer-service/support/ --output-path=./lighthouse/lighthouse-report.html"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^15.0.0",
