    "@ingka/focus": "^2.1.10",
    "@ingka/forms": "^5.0.2",
    "@ingka/hyperlink": "^3.0.5",
    "@ingka/inline-message": "^0.6.13",
    "@ingka/modal": "^0.8.18",
